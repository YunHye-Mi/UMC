        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/video_order_ll"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="15dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent">

            <TextView
                android:id="@+id/video_order_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="최신순"
                android:textSize="14sp"
                android:textColor="@color/black"/>

            <ImageView
                android:id="@+id/video_order_iv"
                android:layout_width="18dp"
                android:layout_height="20dp"
                android:src="@drawable/ic_baseline_arrow_drop_down_24"
                android:scaleType="fitXY"
                app:tint="@color/black" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/video_ll1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="15dp"
            app:layout_constraintTop_toBottomOf="@id/video_order_ll"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent">

            <ImageView
                android:id="@+id/video_mv_iv1"
                android:layout_width="350dp"
                android:layout_height="200dp"
                android:src="@drawable/video_lilac_exp" />

            <TextView
                android:id="@+id/video_my_tv1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="[MV] Coin"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:textSize="14sp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="10dp"/>

            <TextView
                android:id="@+id/video_mv_singer_tv1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="아이유 (IU) | 2021.03.26"
                android:textSize="11sp"
                android:layout_marginStart="5dp" />

        </LinearLayout>

